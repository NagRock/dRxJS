    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@types/animejs": "^2.0.2",
    "@types/d3": "^5.7.2",
    "@types/immutable": "^3.8.7",
    "@types/ramda": "^0.26.16",
    "@types/stacktrace-js": "^0.0.32",
    "animejs": "3.1.0",
    "core-js": "^2.5.4",
    "d3": "^5.9.2",
    "immutable": "^4.0.0-rc.12",
    "ngx-build-plus": "^7.7.6",
    "ramda": "^0.26.1",
    "rxjs": "6.5.2",
    "stackframe": "^1.1.0",
    "stacktrace-gps": "^3.0.3",
    "stacktrace-js": "^2.0.0",
    "svg-pan-zoom": "^3.6.0",
