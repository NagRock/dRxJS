{
  "manifest_version": 2,
  "version": "0.1.0",
  "name": "Doctor RxJS",
  "description": "Doctor RxJS",
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation"
  ],
  "devtools_page": "devtools.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js",
        "runtime.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "/page-script.js",
    "/page-script-vendor.js"
  ]
}
